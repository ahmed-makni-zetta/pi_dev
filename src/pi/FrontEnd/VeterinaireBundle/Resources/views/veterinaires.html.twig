{% extends ":FrontEnd:base.html.twig" %}

{% block body %}
    <div class="gallery team">
        <div class="container">
            <h3 class="agileits-title w3title2">veterinaire</h3>

            <form action="{{ path('front_end_vet') }}" method="post">
                <input type="text" name="recherche" id="recherche" placeholder="search For ...." class="form-control">
                <input type="submit" value="search"   class="btn btn-default">
            </form>
             <div class=" col-md-4 col-md-offset-4  ">
        </div>
            <div class="agile_gallery_grids">
                {% for  i in veterinaires %}
                    <div class="col-sm-4 col-xs-4 col-md-6 agile_gallery_grid">
                        <div class="w3ls_gallery_grid">
                            <a href="{{ path('front_end_show_vet', { 'id': i.id }) }}" class="lsb-preview wthree_p_grid" data-lsb-group="header">
                                <img src="{{ asset('FrontEnd/images/g1.jpg') }}" alt=" " class="img-responsive" />
                                <div class="agileinfo_content_wrap">
                                    <div class="agileits_content">
                                        <h3>Best Pets  {{ i.email  }} </h3>
                                        <p>Ne nam facilisis adolescens faucibus. {{ i.numTel }}</p>
                                        {#{{ i.idmembre }}#}
                                    </div>
                                </div>
                            </a>
                        </div>
                        </br>
                    </div>
                {% endfor %}
            </div>
        </div>



        <div class="navigation text-center">
            {{ knp_pagination_render(veterinaires) }}
        </div>
    </div>
{% endblock %}